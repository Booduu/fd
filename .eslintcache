      {/* isDragActive */}\n                        </div>\n                </div>   \n            }\n            \n        </div>\n        <Cropper \n            objectUrl={objectUrl} \n            open={open}\n            handleClose={handleClose}\n            onChange={onChange}\n        />\n    </>\n     );\n});\n \nexport default connect(state => ({\n    errors: state.apiDataReducer.errors,\n}), {\n})(UploadAndCrop);\n","/Users/boodu/Documents/myapps/fulldub/fd_app/src/components/admin/signup/index.js",[],"/Users/boodu/Documents/myapps/fulldub/fd_app/src/components/admin/signin/index.js",["196","197"],"/Users/boodu/Documents/myapps/fulldub/fd_app/src/components/public/landingPage/index.js",[],"/Users/boodu/Documents/myapps/fulldub/fd_app/src/components/public/shows/index.js",[],"/Users/boodu/Documents/myapps/fulldub/fd_app/src/components/public/bookingContact/index.js",[],"/Users/boodu/Documents/myapps/fulldub/fd_app/src/components/public/discography/index.js",["198"],"/Users/boodu/Documents/myapps/fulldub/fd_app/src/components/public/home/index.js",["199","200"],"\nimport React, { useEffect } from 'react';\nimport { connect } from 'react-redux';\nimport style from './Home.module.scss';\n\nconst Home = ({\n    setIsHome,\n    albums,\n}) => {\n    \n    useEffect(() => {\n        setIsHome(true);\n        return () => {\n            setIsHome(false);\n        }\n    }, []);\n\n    return ( \n    \n            <div className={style.container}>\n                 {albums && albums[0]?.title && (\n                <a href={albums[0].downloadLink} target=\"_blank\">\n                    <div className={style.titles}>\n                        <h1>{albums[0].title}</h1>\n                        <h3>NEW ALBUM AVAILABLE NOW</h3>\n                    </div> \n                </a>     \n                )}\n            </div>\n        \n     );\n}\n \nexport default connect(state => ({\n    albums: state.apiDataReducer.albums,\n}))(Home);","/Users/boodu/Documents/myapps/ful